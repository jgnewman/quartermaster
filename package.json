{
  "name": "quartermaster",
  "version": "0.0.0-semantic",
  "description": "A shared component library",
  "main": "index.js",
  "types": "index.d.ts",
  "sideEffects": [
    "**/*.css",
    "**/*.styl"
  ],
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --http --color --progress --inline --mode development --offline",
    "bundlesize": "NODE_ENV=production webpack --http --color --progress --inline --mode production --offline",
    "build": "node scripts/build",
    "release": ". ./.env && node scripts/release",
    "component": "node scripts/create-component"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgnewman/quartermaster.git"
  },
  "author": "John Newman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgnewman/quartermaster/issues"
  },
  "homepage": "https://github.com/jgnewman/quartermaster#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@types/node": "^13.9.3",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
