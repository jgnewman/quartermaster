    "exec-sh": "^0.3.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "import-glob-loader": "^1.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-fetch": "^2.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prompts": "^2.3.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
